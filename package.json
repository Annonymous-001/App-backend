{
  "name": "school-management-backend",
  "version": "1.0.0",
  "description": "Backend API for School Management Mobile App",
  "main": "dist/index.js",
    "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "npx prisma generate && tsc",
    "start": "node dist/index.js",
    "postinstall": "npx prisma generate",
    "db:generate": "npx prisma generate",
    "db:push": "npx prisma db push",
    "db:migrate": "npx prisma migrate dev",
    "db:seed": "tsx src/seed.ts"
  },
  "dependencies": {
    "@clerk/clerk-sdk-node": "^4.13.14",
    "@clerk/express": "^1.7.24",
    "@prisma/client": "^5.7.1",
    "@types/jsonwebtoken": "^9.0.10",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.5",
    "prisma": "^5.7.1",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3"
  }
}
